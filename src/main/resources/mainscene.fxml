<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.TextArea?>
<VBox alignment="CENTER" prefHeight="800.0" prefWidth="800.0" spacing="30.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/20.0.1" fx:controller="com.example.javapoker.Graphics.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

    <Label fx:id="mainLabelText" text="Welcome to the Main Scene!">
        <font>
            <Font name="Arial" size="21.0" />
        </font>
    </Label>

    <Button fx:id="goBackButton" prefHeight="54.0" prefWidth="129.0" text="Go Back" onAction="#goBack">
        <font>
            <Font name="System Bold Italic" size="22.0" />
        </font>
    </Button>

    <TextArea fx:id="outputArea" editable="false" prefHeight="200" prefWidth="300" wrapText="true"/>
    <TextField fx:id="inputField" onAction="#handleInput"/>
    <Button text="Submit" onAction="#handleInput"/>

</VBox>